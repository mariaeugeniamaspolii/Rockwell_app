{
  "expo": {
    "name": "Rockwell app",
    "slug": "rockwell-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/img/rockwellLogo.png",
    "userInterfaceStyle": "light",
    "scheme": "demo",
    "splash": {
      "image": "./assets/img/rockwellLogo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/img/rockwellLogo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.mariamaspoli.rockwellapp"
    },
    "web": {
      "favicon": "./assets/img/rockwellLogo.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "ae9a61e5-91d8-4fbd-bbf0-4e9c13af14cb"
      }
    },
    "owner": "mariamaspoli"
  }
}
